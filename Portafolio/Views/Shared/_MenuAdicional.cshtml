@model Usuario

<div class="options-container-fixed">
    <div class="options">
        <div class="row">
            <div class="col-sm-12">
                <a href="~/default/ExportarPdf" target="_blank" class="btn btn-danger">
                    <i class="fa fa-file-pdf-o"></i> <span class="hidden-xs hidden-sm">Descargar en PDF</span>
                </a>
                <button class="btn btn-primary">
                    <i class="fa fa-print"></i> <span class="hidden-xs hidden-sm">Imprimir</span>
                </button>
                <button class="btn btn-primary" data-toggle="modal" data-target="#mContacto">
                    <i class="fa fa-envelope-o"></i> <span class="hidden-xs hidden-sm">Contactar</span>
                </button>
                @if (Model.Linkedin != null && Model.Linkedin != "")
                {
                    <a target="_blank" href="@Model.Linkedin" title="Linkedin" class="btn btn-default"><i class="fa fa-linkedin"></i></a>
                }
                @if (Model.Github != null && Model.Github != "")
                {
                    <a target="_blank" href="@Model.Github" title="Facebook" class="btn btn-default"><i class="fa fa-github"></i></a>
                }
                @if (Model.Youtube != null && Model.Youtube != "")
                {
                    <a target="_blank" href="@Model.Youtube" title="Facebook" class="btn btn-default"><i class="fa fa-youtube-play"></i></a>
                }
                @if (Model.Twitter != null && Model.Twitter != "")
                {
                    <a target="_blank" href="@Model.Twitter" title="Facebook" class="btn btn-default"><i class="fa fa-twitter"></i></a>
                }

            </div>
        </div>
    </div>
</div>
@Html.Partial("~/views/shared/_Contacto.cshtml", new Portafolio.DTO.ContactoDTO())